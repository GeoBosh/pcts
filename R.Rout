        inew <- which(!apply(cmp, 2, any))
        irem <- which(!apply(cmp, 1, any))
        icom <- which(!(seq_along(x) %in% c(inew, irem)))
    }
    list(i_new = inew, i_removed = irem, i_common = icom)
}
