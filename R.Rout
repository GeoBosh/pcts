Browse[2]> .acv2acr
function(x, maxlag, ...){  ## TODO: include arg. lag_0 !
    ## compute the variances in a separate call
    ##  (I don't remember why, maybe initially "..." was allowed to
    ##   subset, raising the possibility that the variances
    ##   are not in 'acv', see below).
    sd <- sqrt(autocovariances(x, 0)[[0]])

    if(missing(maxlag))
        maxlag <- maxLag(x)

    fac <- pc.sdfactor(sd, maxlag)[ , 1 + (0:maxlag)] # "1+" since "matrix"
    acv <- autocovariances(x, maxlag, ...)

    acv / fac
}
